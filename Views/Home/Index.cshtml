@model List<ITI_MVC.Controllers.HomeController.RouteInfo>

<h2>Available Routes</h2>
<ul>
    @foreach (var route in Model)
    {
        <li>
            <a href="@Url.Action(route.Action, route.Controller)">
                @($"{route.Controller}/{route.Action}")
            </a>
        </li>
    }
</ul>
