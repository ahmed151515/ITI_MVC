@model List<string>
<div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">

<ul class="navbar-nav flex-grow-1">
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </li>
@foreach (var controllerName in Model)
{
        <li  class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="@controllerName" asp-action="Index">@controllerName</a>
        </li>
        
    
}
</ul>
</div>